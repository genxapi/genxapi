{
  "name": "@eduardoac/generate-api-client",
  "version": "0.1.4",
  "description": "CLI for generating multiple API clients from pluggable templates.",
  "license": "Apache-2.0",
  "type": "module",
  "bin": {
    "generate-api-client": "./bin/generate-api-client.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin",
    "schemas"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "restricted"
  },
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf dist",
    "lint": "eslint 'src/**/*.ts'",
    "npm-publish": "npm run publish:npm",
    "publish:npm": "npm publish --access restricted",
    "publish:npm-public": "echo \"Public publish disabled until GA\"",
    "publish:github": "npm publish --registry https://npm.pkg.github.com",
    "prebuild": "npm run clean",
    "test": "vitest run"
  },
  "dependencies": {
    "@eduardoac/orval-api-client-template": "0.1.0",
    "@eduardoac/kubb-api-client-template": "0.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "cosmiconfig": "^9.0.0",
    "fs-extra": "^11.2.0",
    "octokit": "^4.0.0",
    "ora": "^8.0.0",
    "pathe": "^1.1.2",
    "execa": "^9.3.0",
    "merge-deep": "^3.0.3",
    "yaml": "^2.5.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.0.0",
    "@types/yaml": "^1.9.7",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.13.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "rimraf": "^5.0.10",
    "rollup": "^4.34.0",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  }
}
